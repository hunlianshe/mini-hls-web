{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES6",
    "allowJs": false,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "removeComments": true,
    "pretty": true,
    "strictPropertyInitialization": true,
    "typeRoots": [
      "./typings"
    ]
  },
  "include": [
    "miniprogram/**/*.ts", "miniprogram/components/pageContainer/pageContainer.js", "miniprogram/components/miniPrograss/miniPrograss.js", "miniprogram/utils/sha1.js", "miniprogram/pages/qsqyResult/qsqyResult.js", "miniprogram/pages/realXzysDetail/realXzysDetail.js", "miniprogram/pages/bar/index.js", "miniprogram/pages/bar/index.js", "miniprogram/components/userInfoCard/userInfoCard.js"
  ],
  "exclude": [
    "node_modules",
    "miniprogram_dist",
    "**/*.spec.ts"
  ]
}
