<!--components/userInfoCard.wxml-->
<view class="userInfoCard">
  <view class="avatarUrl">
    <image src="{{userInfo.avatarUrl}}" mode="aspectFit" />
  </view>
  <view style="padding: 0 20rpx; flex: 1;">
    <view style="font-size: 36rpx; color: #030303;">{{userInfo.nickName}}</view>
    <view style="font-size: 24rpx; color: #9092A5">信息待完善</view>
  </view>
  <hls-icon iconName="arrowRed" />
</view>
