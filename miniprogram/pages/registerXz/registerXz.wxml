<view>
  <page-container>
    <view class="registerXz">
      <view class="subtitle">
        你的星座
      </view>
      <view class="constellation">
        <view class="block" wx:for="{{constellationList}}" wx:key="{{item.ch}}" 
        bindtap="select" data-constellation="{{item.ch}}">
          <image src="../../public/image/Signs/{{item.en}}.png" mode="aspectFit"/>
          <view class="name">
            {{item.ch}}
          </view>
        </view>
      </view>
      <view class="footer-container">
        <button class="common-btn" catchtap="next">下一步</button>
      </view>
    </view>
    
  </page-container>
</view>
