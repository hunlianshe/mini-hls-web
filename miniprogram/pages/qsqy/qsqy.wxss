.qsqyPage {
  background: linear-gradient(to right, #F2E2CC, #E9D4BB);
  position: absolute;
  width: 100%;
  height: 100%;
}

.qsqyPage .title {
  font-size: 46rpx;
  padding: 30rpx 0;
  text-align: center;
}

.qsqyPage .info {
  font-size: 30rpx;
  color: #666666;
  padding: 30rpx 0;
  text-align: center;
  margin-bottom: -80rpx;
  z-index: 1;
  position: absolute;
  width: 100%;
}

.qsqyPage .bg-img {
  width: 100%;
  height: 90%;
  position: absolute;
  bottom: 0;
}

@media  screen and (min-height: 800px) {
  .qsqyPage .bg-img {
    height: 70%;
  }
}

.qsqyPage .bg-img image {
  width: 100%;
}


.qsqyPage .tip {
  position: absolute;
  bottom: 30rpx;
  left: 50rpx;
  font-size: 22rpx;
  font-weight: 300;
  color: #666666;
}