.matchmakerPage {
  color: #4A4A4A;
  font-size: 28rpx;
}
.matchmakerPage .matcherCard {
  display: flex;
  align-items: center;
  margin: 32rpx 64rpx;
  padding: 32rpx;
  font-size: 36rpx; 
  color: #A1261B;
  background: #F9CFC9;
  border-radius: 8rpx;
  border: 2rpx solid #BC635B;
}

.matchmakerPage .matcherCard .left-info {
  display: flex;
  align-items: center;
}

.matchmakerPage .service-title {
  text-align: center;
  margin: 64rpx 0;
}

.matchmakerPage .service-title image{
  height: 36rpx;
}

.matchmakerPage .service-item {
  display: flex;
  align-items: center;
  margin: 32rpx 64rpx;
}

.matchmakerPage .service-item-name {
  color: #651209;
  font-size: 30rpx;
}

.matchmakerPage .service-item-desc {
  color: #999999;
  font-size: 24rpx;
  line-height: 28rpx;
  padding-top: 10rpx;
  font-weight: 300;
}

.matchmakerPage .footer {
  position: absolute;
  bottom: 40rpx;
  font-size: 32rpx;
  color: #ffffff;
  padding-left: 40rpx;
}